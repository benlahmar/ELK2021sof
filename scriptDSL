GET bank2022/_mapping

GET bank2022/_search
{
  "query": {
    "bool": {
      "must": [
        {
          "range": {
            "balance": {
              "gte": 8000
              
            }
          }
        },
        {
          "range": {
            "age": {
              "gte": 25
              
            }
          }
        }
      ],
      "filter": [
        {
          "script": {
            "script": {
              
              "source": "doc['operations.amount'].value > 100"
              
            }
          }
        }
      ]
    }
  }
}

