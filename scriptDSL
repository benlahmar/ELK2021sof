PUT myblog2022017
{
  "settings": { 
    "analysis": {
      "analyzer": {
        "titleanlyzer":
        {
          "type":"custom",
          "tokenizer":"standard",
          "filter":["stopfilter","lowercase","synonymfilter"]
          
        },
        "descriptionanlyzer":{
          "type":"custom",
          "tokenizer":"standard",
           "filter":["stopfilter","lowercase"]
        },
        "contenentanlyzer":{
          "type":"custom",
           "char_filter":["html_strip"],
          "tokenizer":"standard",
          "filter":["stopfilter","lowercase"]
        }
        
      },
      "tokenizer": {
        "mytoken":{
          "type":"standard"
        }
      },
      "filter":{
        "synonymfilter":{
          "type":"synonym",
          "synonyms":["officiel  => officiel, églementaire, administratif"]
        },
         "stopfilter":{
          "type":"stop",
          "stopwords":"_french_"
        }
      }
    }
     
      
    }
  }



